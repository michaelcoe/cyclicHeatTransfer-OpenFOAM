// Temperatures
bulkTemp            uniform -30;//293.15;
wallTemp            uniform 0;//323.15;

// Cyclic constraint
flowUbar          (1.65e-03 0.0 0.0);

// Thermal Properties
thermalBeta         3e-03;
regularPr           0.7;
turbulentPr         0.85;
lambdaGuess         0.5387;
domainLength        1.0;
inletHeight	        0.5;		    
gamma               0.01;

